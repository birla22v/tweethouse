<p id="notice"><%= notice %></p>

<h1>Welcome to TweetHouse!</h1>

<ul>
  <%  @users.each do |user| %>
  <li>Hi there, <%= user.username %>.
    <%=  link_to "Your Shouts", user_shouts_path(user) %> |
    <%=  link_to "Delete Account", user_path(user), method: :delete %> |
    <%=  link_to "New Shout", new_user_shout_path(user) %>
  </li>
  <%  end  %>
</ul>

<%= link_to 'Back', :back %>| <%= link_to 'New User', new_user_path %>
